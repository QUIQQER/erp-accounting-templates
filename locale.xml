        <!-- SEPA mandate -->
        <locale name="sepaMandate.title">
            <de><![CDATA[SEPA-Lastschriftmandat]]></de>
            <en><![CDATA[SEPA direct debit mandate]]></en>
        </locale>
        <locale name="sepaMandate.header.creditor">
            <de><![CDATA[Gläubiger-Identifikationsnummer]]></de>
            <en><![CDATA[Creditor Identifier]]></en>
